    "gulp-uglify-es": "^2.0.0",
    "jest": "^26.6.3",
    "node-html-parser": "^4.1.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
