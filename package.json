    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "commander": "^7.2.0",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "faker": "^5.5.3",
    "hammerjs": "^2.0.8",
    "image-size": "^0.9.7",
    "jimp": "^0.16.1",
    "joi": "^17.4.0",
    "jquery": "^3.6.0",
    "mariadb": "^2.5.3",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nestjs-console": "^4.0.0",
    "node-sql": "^1.1.5",
    "nodemailer": "^6.5.0",
    "p-iteration": "^1.1.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "slug": "^4.0.4",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/joi": "^17.2.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.31",
