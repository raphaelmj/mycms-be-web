  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "drop": "npm run typeorm schema:drop",
    "migrate": "npm run typeorm schema:sync",
    "users": "ts-node -r tsconfig-paths/register src/console.ts create-users",
    "positions": "ts-node -r tsconfig-paths/register src/console.ts create-positions",
    "pages": "ts-node -r tsconfig-paths/register src/console.ts create-pages",
    "categories": "ts-node -r tsconfig-paths/register src/console.ts create-categories",
    "variants": "ts-node -r tsconfig-paths/register src/console.ts create-variants",
    "departments": "ts-node -r tsconfig-paths/register src/console.ts create-departments",
    "offices": "ts-node -r tsconfig-paths/register src/console.ts create-offices",
    "contacts": "ts-node -r tsconfig-paths/register src/console.ts create-contacts",
    "investitions": "ts-node -r tsconfig-paths/register src/console.ts create-investitions",
    "articles": "ts-node -r tsconfig-paths/register src/console.ts create-articles",
    "messages": "ts-node -r tsconfig-paths/register src/console.ts create-messages",
    "associations": "ts-node -r tsconfig-paths/register src/console.ts make-associations",
    "articles-custom": "ts-node -r tsconfig-paths/register src/console.ts create-custom-articles",
    "articles-status": "ts-node -r tsconfig-paths/register src/console.ts articles-status",
    "seeds": "npm run users && npm run positions && npm run categories && npm run departments && npm run variants && npm run pages && npm run offices && npm run contacts && npm run investitions && npm run articles && npm run messages && npm run articles-custom && npm run associations && npm run create-popups",
    "db:make": "npm run drop && npm run migrate && npm run seeds",
    "invests-contacts": "ts-node -r tsconfig-paths/register src/console.ts investitions-contacts",
    "invests-pin-contacts": "ts-node -r tsconfig-paths/register src/console.ts investitions-pin-contacts",
    "galleries-refactor": "ts-node -r tsconfig-paths/register src/console.ts galleries-refactor",
    "link-invest-refactor": "ts-node -r tsconfig-paths/register src/console.ts link-invest-refactor",
    "invests-merge": "ts-node -r tsconfig-paths/register src/console.ts invests-merge",
    "invests-aliases": "ts-node -r tsconfig-paths/register src/console.ts invests-aliases",
    "articles-aliases-id-int": "ts-node -r tsconfig-paths/register src/console.ts create-articles-aliases-id-int",
    "refactor-messages-json": "ts-node -r tsconfig-paths/register src/console.ts refactor-messages-json",
    "articles-dates-to-published": "ts-node -r tsconfig-paths/register src/console.ts articles-dates-to-published",
    "invest-map-files": "ts-node -r tsconfig-paths/register src/console.ts invest-maps-more",
    "offices-map-files": "ts-node -r tsconfig-paths/register src/console.ts make-offices-map-files",
    "invest-progress-gallery": "ts-node -r tsconfig-paths/register src/console.ts invest-progress-gallery-intro-image",
    "invest-bool-custom-table": "ts-node -r tsconfig-paths/register src/console.ts invest-bool-custom-table",
    "create-popups": "ts-node -r tsconfig-paths/register src/console.ts create-popups",
    "full-refactor-articles": "npm run articles-aliases-id-int && npm run articles-dates-to-published && npm run articles-status",
    "make-meta": "ts-node -r tsconfig-paths/register src/console.ts make-meta",
    "images-to-gallery": "ts-node -r tsconfig-paths/register src/console.ts find-images-sizes",
    "images-to-gallery-full-path": "ts-node -r tsconfig-paths/register src/console.ts find-images-sizes -fp 1",
    "parse-html-table-niemcewicz": "ts-node -r tsconfig-paths/register src/console.ts parse-html-tables -hp /static/tables/niemcewicza-table.html",
    "parse-html-table-poborzanska": "ts-node -r tsconfig-paths/register src/console.ts parse-html-tables -hp /static/tables/poborzanska-table.html",
    "parse-html-table-swiatowida": "ts-node -r tsconfig-paths/register src/console.ts parse-html-tables -hp /static/tables/switowida-table.html",
    "update-contacts-jsons": "ts-node -r tsconfig-paths/register src/console.ts update-contacts-jsons",
    "refactor-pages-slides-jsons": "ts-node -r tsconfig-paths/register src/console.ts refactor-sliders"
  },
  "dependencies": {
